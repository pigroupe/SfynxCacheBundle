{
  "name": "sfynx-project/tool-cache-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides classes to cache handlers (memcache, files)",
  "keywords": ["tool", "cache", "memcache", "file", "ttl"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxCacheBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8|~3.0",
    "cache/psr-6-doctrine-bridge": "^3.0",
    "cache/adapter-bundle": "^1.2",
    "cache/prefixed-cache": "^1.0",
    "cache/taggable-cache": "^1.0",
    "cache/predis-adapter": "^1.0",
    "cache/doctrine-adapter": "^1.0",
    "doctrine/cache": "1.*"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\CacheBundle": "" }
  },

  "target-dir": "Sfynx/CacheBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
